{
    "name": "tichu-score-tracker",
    "private": true,
    "version": "1.2.8-preview",
    "type": "module",
    "homepage": "https://polarode.github.io/tichu-score-tracker",
    "scripts": {
        "dev": "vite",
        "build": "npm run lingui:compile && tsc -b && vite build",
        "postbuild": "cp dist/index.html dist/404.html",
        "predeploy": "npm run build && npm run postbuild",
        "deploy": "gh-pages -d dist",
        "lint": "eslint .",
        "preview": "vite preview",
        "format": "biome format --write",
        "lingui:extract": "lingui extract",
        "lingui:compile": "lingui compile",
        "lingui:translate": "npm run lingui:extract && npm run lingui:compile"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@lingui/core": "^5.3.2",
        "@lingui/macro": "^5.3.2",
        "@lingui/react": "^5.3.2",
        "@lingui/vite-plugin": "^5.3.2",
        "@mui/icons-material": "^7.1.0",
        "@mui/material": "^7.1.0",
        "@supabase/supabase-js": "^2.49.8",
        "classnames": "^2.5.1",
        "react": "^19.1.0",
        "react-cat-paws": "^0.3.0",
        "react-dom": "^19.1.0",
        "react-router-dom": "^7.6.0",
        "react-toastify": "^11.0.5"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@eslint/js": "^9.25.0",
        "@lingui/cli": "^5.3.2",
        "@types/react": "^19.1.2",
        "@types/react-dom": "^19.1.2",
        "@vitejs/plugin-react": "^4.4.1",
        "babel-plugin-macros": "^3.1.0",
        "eslint": "^9.25.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.19",
        "gh-pages": "^6.3.0",
        "globals": "^16.02.0",
        "supabase": "^2.24.3",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.30.1",
        "vite": "^6.3.5"
    }
}
